{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "interface-name": [
      false
    ],
    "member-ordering": [
      true,
      {
        "order": [
          "static-field",
          "instance-field",
          "constructor",
          "public-instance-method",
          "protected-instance-method",
          "private-instance-method"
        ]
      }
    ],
    "no-console": [false],
    "no-string-literal": false,
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "switch-default": false,
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ]
  }
}
